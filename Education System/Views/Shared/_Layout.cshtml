                            <li><a href="#Feedback">Feedback</a></li>
                           
                        </ul>
                        <ul class="pull-right sm-pull-nonelist-inline nav-side-icon-list">
                            <li>
                               
                                @using (Html.BeginForm("search1", "Home", FormMethod.Get))

                                {


                                    <form method="post">
                                        <!-- search box -->
                                        <div class="aa-search-box">

                                            <input type="text" name="search" id="" placeholder="Search here ex.'CSS'">
                                            <button type="submit" value="search"><span class="fa fa-search"></span></button>

                                        </div>
                                        <!-- / search box -->
                                    </form>
                                }
                            </li>
                        </ul>
