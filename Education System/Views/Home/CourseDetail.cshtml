    <!-- CSS | Theme Color -->
    @*<link href="css/colors/theme-skin-color-set1.css" rel="stylesheet" type="text/css">*@
    @*<link href="~/css/colors/theme-skin-color-set2.css" rel="stylesheet" />*@
    <link href="~/css/colors/theme-skin-color-set1.css" rel="stylesheet" />
</head>
<body>
    <form action="@Url.Action("addtocart")" method="post">
        <div class="main-content">
            <!-- Section: inner-header -->
            <section class="inner-header divider layer-overlay overlay-theme-colored-7" data-bg-img="/Images/bg/bg1.jpg">
                @*<img src="~/Images/bg/bg1.jpg" />*@
                <div class="container pt-120 pb-60">
                    <!-- Section Content -->
                    <div class="section-content">
                        <div class="row">
                            <div class="col-md-6">
                                <h2 class="text-white font-36">Course Details</h2>
                                <ol class="breadcrumb text-left mt-10 white">
                                    <li><a href="#">@Html.ActionLink("Home", "Index")</a></li>
                                    <li class="active">Current Page</li>
                                </ol>
                            </div>
                        </div>
                    </div>
                </div>
            </section>

            <!-- Section: service-->
            <section>
                <div class="container">
                    <div class="row">
                        <div class="col-md-8 blog-pull-right">
                            <div class="single-service">
                                @*<img src="images/course/lg6.jpg" alt="">*@
                                <img src="@Url.Content(Model.Image)" height="450px" width="750px" />
                                <h3 class="text-theme-colored line-bottom text-theme-colored">@Model.Name</h3>
                                <ul class="list-inline mb-15">
                                    <li>
                                        <i class="pe-7s-ribbon text-theme-colored2 font-48"></i>
                                        <div class="pull-right ml-5">
                                            <span>Category</span>
                                            <h5 class="mt-0">Web Design</h5>
                                        </div>
                                    </li>
                                    <li>
                                        <i class="pe-7s-cash text-theme-colored2 font-48"></i>
                                        <div class="pull-right ml-10">
                                            <span>Course Price</span>
                                            <h5 class="mt-0">$@Model.Price</h5>
                                        </div>

                                    </li>
                                </ul>
                                <input type="hidden" name="id" value="@Model.id" />
                                <input type="hidden" name="qty" id="qqet" value="1" />
                                <button type="submit" class="btn btn-default aa-review-submit">Add To Cart</button>

                                <p>@Model.Description</p>
                                @*<p>Lorem ipsum dolor sit amet, consectetur adipisicing elit. Labore voluptatem officiis quod animi possimus a, iure nam sunt quas aperiam non recusandae reprehenderit, nesciunt cumque pariatur totam repellendus delectus? Maxime quasi earum nobis, dicta, aliquam facere reiciendis, delectus voluptas, ea assumenda blanditiis placeat dignissimos quas iusto repellat cumque.</p>*@
                                @*<blockquote class="bg-silver-light">Lorem ipsum dolor sit amet, consectetur adipisicing elit. Labore voluptatem officiis quod animi possimus a, iure nam sunt quas aperiam non recusandae reprehenderit, nesciunt cumque pariatur totam.</blockquote>*@
                                @*<p>Lorem ipsum dolor sit amet, consectetur adipisicing elit. Dolore suscipit, inventore aliquid incidunt, quasi error! Natus esse rem eaque asperiores eligendi dicta quidem iure, excepturi doloremque eius neque autem sint error qui tenetur, modi provident aut, maiores laudantium reiciendis expedita. Eligendi</p>*@
                                <ul id="myTab" class="nav nav-tabs mt-30">
                                    <li class="active"><a href="#tab1" data-toggle="tab">Description</a></li>
                                    <li><a href="#tab2" data-toggle="tab">Course Info</a></li>
                                    @*<li><a href="#tab3" data-toggle="tab">Teachers</a></li>*@
                                    @*<li><a href="#tab4" data-toggle="tab">Reviews</a></li>
                                    <li><a href="#tab5" data-toggle="tab">Comments</a></li>*@
                                </ul>
                                <div id="myTabContent" class="tab-content">
                                    <div class="tab-pane fade in active" id="tab1">
                                        <h4 class="line-bottom-theme-colored-2 mb-15">Course Details</h4>
                                        <p>@Model.Course_Details</p>
                                        @*<p>@Model.</p>*@
                                        <h4 class="line-bottom-theme-colored-2 mt-20 mb-10">Requirements</h4>
                                        <ul class="list theme-colored2 angle-double-right">
                                            <li>@Model.Requirements</li>
                                            @*<li>Lorem ipsum, consectetur adipisicing elit</li>
                                            <li>Lorem consectetur adipisicing elit</li>
                                            <li>Lorem consectetur adipisicing elit</li>
                                            <li>Lorem consectetur adipisicing elit</li>*@
                                        </ul>
                                        <h4 class="line-bottom-theme-colored-2 mt-20 mb-10">Course Syllabus</h4>
                                        <ul class="list theme-colored2 paper">
                                            <li>@Model.Course_Syllabus</li>
                                            @*<li>Lorem consectetur adipisicing elit</li>
                                            <li>Lorem consectetur adipisicing elit</li>
                                            <li>Lorem consectetur adipisicing elit</li>
                                            <li>Lorem consectetur adipisicing elit</li>
                                            <li>Lorem consectetur adipisicing elit</li>*@
                                        </ul>
                                    </div>
                                    <div class="tab-pane fade" id="tab2">
                                        <h4 class="line-bottom-theme-colored-2 mb-15">Course Information</h4>
                                        <ul class="course-info-list font-14 mt-20">
                                            <li>
                                                <span class="course-info-title">
                                                    <i class="pe-7s-timer font-26 vertical-align-middle text-theme-colored2 mr-10"></i>Course Duration:
                                                </span>
                                                <span class="course-info-details">@Model.Duration Month</span>
                                            </li>
                                            <li>
                                                <span class="course-info-title">
                                                    <i class="pe-7s-diamond font-26 vertical-align-middle text-theme-colored2 mr-10"></i>Total Credits:
                                                </span>
                                                <span class="course-info-details">@Model.Credits</span>
                                            </li>
                                            <li>
                                                <span class="course-info-title">
                                                    <i class="pe-7s-umbrella font-26 vertical-align-middle text-theme-colored2 mr-10"></i>Student Capacity:
                                                </span>
                                                <span class="course-info-details">Max @Model.seats Students</span>
                                            </li>

                                            <li>
                                                <span class="course-info-title">
                                                    <i class="pe-7s-users font-26 vertical-align-middle text-theme-colored2 mr-10"></i>Course Lessons
                                                </span>
                                                <span class="course-info-details">@Model.Lessons Lessons</span>
                                            </li>
                                        </ul>

                                    </div>

                                </div>
                            </div>
                        </div>
                        <div class="col-sm-12 col-md-4">
                            <div class="sidebar sidebar-left mt-sm-30 ml-40">
                                <div class="widget border-1px bg-silver-light p-15">
                                    <h4 class="widget-title line-bottom-theme-colored-2">Search box</h4>
                                    @*<div class="search-form">
                                        <form>
                                            <div class="input-group">
                                                <input type="text" placeholder="Click to Search" class="form-control search-input">
                                                <span class="input-group-btn">
                                                    <button type="submit" class="btn search-button text-white bg-theme-colored2"><i class="fa fa-search"></i></button>
                                                </span>
                                            </div>
                                        </form>
                                    </div>*@
                                </div>
                                <div class="widget">
                                    <h4 class="widget-title line-bottom">Service <span class="text-theme-colored2">List</span></h4>
                                    <div class="services-list">
                                        <ul class="list list-border">
                                            <li class="active"><a href="page-service-details.html">Computer Science</a></li>
                                        </ul>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </section>

        </div>
    </form>
        <!-- external javascripts -->
        @*<script src="js/jquery-2.2.4.min.js"></script>*@
        <script src="~/js/jquery-2.2.4.min.js"></script>
        @*<script src="js/jquery-ui.min.js"></script>*@
        <script src="~/js/jquery-ui.min.js"></script>
        @*<script src="js/bootstrap.min.js"></script>*@
        <script src="~/js/bootstrap.min.js"></script>
        <!-- JS | jquery plugin collection for this theme -->
        @*<script src="js/jquery-plugin-collection.js"></script>*@
        <script src="~/js/jquery-plugin-collection.js"></script>
        <!-- JS | Custom script for all pages -->
        @*<script src="js/custom.js"></script>*@
        <script src="~/js/custom.js"></script>
        @*<script src="js/extra.js"></script>*@
        <script src="~/js/extra.js"></script>
        
</body>
</html>
