<body>
    <section id="menu">
        <div class="container">
            <div class="menu-area">
                <!-- Navbar -->
                @*@Html.Partial("_navbar")*@
            </div>
        </div>
    </section>



    <table class="table">
        <tr>
            <section id="courses" class="bg-silver-light">
                <div class="container pb-40">
                    
                    <div class="row mtli-row-clearfix">
                        
                            @foreach(var item in Model)
                            {
                                @*<div class="item">*@
                                    <div class="course-single-item style2 text-center mb-40">
                                        <a class="aa-product-img" href="@Url.Action("product_list", "Home",new { id = item.id})">
                                            <div class="course-thumb">
                                                @*<img class="img-fullwidth" alt="" src="images/course/sm1.jpg">*@
                                                @*@if (item.Image != null)
                                                {*@
                                                    <img class="img-fullwidth" alt="" src="@Url.Content(item.Image)" height="650" width="100" />
                                                @*}*@
                                                @*else
                                                {
                                                    <img class="img-fullwidth" src="~/Images/default-course-thumbnail.png" />
                                                }*@
                                                @*<img class="img-fullwidth" src="~/Images/default-course-thumbnail.png" />*@

                                                <div class="price-tag">@item.Price INR</div>
                                            </div>
                                            <div class="course-details bg-white border-1px clearfix p-15 pt-15">
                                                <div class="course-top-part">
                                                    <h4 class="line-bottom-centered mt-20">@item.Name</h4>
                                                    <a href="#"><h7>@Html.ActionLink("Click here for Course", "CourseDetail", new { id = item.id })</h7></a>

                                                    <ul class="list-inline">
                                                        <li class="review-stars text-theme-colored2">
                                                            <i class="fa fa-star"></i>
                                                            <i class="fa fa-star"></i>
                                                            <i class="fa fa-star"></i>
                                                            <i class="fa fa-star"></i>
                                                            <i class="fa fa-star"></i>
                                                        </li>
                                                    </ul>
                                                </div>
                                                <div class="author-thumb">
                                                    <img src="images/course/xs1.jpg" alt="" class="img-circle img-thumbnail">
                                                </div>
                                                <p class="course-description mt-5">@item.Description</p>
                                            </div>
                                            <div class="course-meta bg-theme-colored">
                                                <ul class="list-inline text-white">
                                                    <li><i class="fa fa-calendar text-theme-colored2 mr-5"></i>@item.Duration hours</li>
                                                    <li><i class="fa fa-book text-theme-colored2 mr-5"></i>@item.Lessons Lessons</li>
                                                    <li><i class="fa fa-clock-o text-theme-colored2 mr-5"></i>@item.seats Seats</li>
                                                </ul>
                                            </div>
                                    @*</div>*@
                                </div>
                            }
                        </div>
                    </div>
                
            </section>
           


        </tr>
    </table>
